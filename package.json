{
  "name": "Socket.io-Chat-APP-Example",
  "version": "1.0.4",
  "type": "module",
  "description": "Example Node.js Chat APP based on socket.io",
  "main": "app.js",
  "dependencies": {
    "assert": "^2.0.0",
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
